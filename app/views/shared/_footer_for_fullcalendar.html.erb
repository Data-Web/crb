<div class="footer_for_fullcalendar">
  <br/>
  <table>
    <tr>
      <% @rooms.each do |room| %>
        <td>
          <div class="square" style="background-color:<%= room.color %>;"></div>
        </td>
        <td><b><%= room.name %></b></td>
      <% end %>
      <td>
        <div class="square past-events"></div>
      </td>
      <td><b><%= t("footer.past_events") %></b></td>
    </tr>
  </table>
</div>
